{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "mapRoot": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@common/*": [
        "./src/app/@common/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@core/*": [
        "./src/app/core/*"
      ],
      "@environments/*": [
        "./src/environments/*"
      ],
      "@auth/*": [
        "./src/app/shared/auth/*"
      ]
    }
  }
}
